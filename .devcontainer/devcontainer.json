{
  "name": "Python Dev Container",
  "image": "mcr.microsoft.com/devcontainers/python:3.8",
  "postCreateCommand": "pip install pandas matplotlib seaborn",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance"
      ]
    }
  },
  "forwardPorts": [],
  "portsAttributes": {
    "8000": {
      "label": "Application",
      "onAutoForward": "notify"
    }
  }
}

  